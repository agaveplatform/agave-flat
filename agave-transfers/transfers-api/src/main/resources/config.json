{
  "transfertask.http.port": 8085,
  "transfertask.jwt.auth": true,
  "transfertask.jwt.private_key": "src/main/resources/private_key.pem",
  "transfertask.jwt.public_key": "src/main/resources/public_key.pem",
  "transfertask.jdbc.database": "${foundation.db.database}",
  "transfertask.jdbc.username": "${foundation.db.username}",
  "transfertask.jdbc.password": "${foundation.db.password}",
  "transfertask.jdbc.driver_class": "${foundation.db.driver}",
  "transfertask.jdbc.url": "${foundation.db.url}",
  "transfertask.jdbc.max_pool_size": 10,
  "transfertask.db.queue": "transfertaskdb.queue",
  "transfertasks.sqlqueries.resource.file": "db-queries.yml",
  "transfertask.max.tries": 3
}