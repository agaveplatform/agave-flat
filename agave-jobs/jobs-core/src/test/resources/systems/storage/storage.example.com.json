{
   "id": "storage.example.com",
   "name": "SFTP Example",
   "status": "UP",
   "type": "STORAGE",
   "description": "Example storage sytem accessible via sftp.",
   "site": "example.com",
   "storage": {
      "host": "${sftp.host}",
      "port": ${sftp.port},
      "protocol": "SFTP",
      "rootDir": "/",
      "homeDir": "/home/testuser",
      "auth": {
         "username": "testuser",
         "password": "testuser",
         "type": "PASSWORD"
      }
   },
   "default": false
}