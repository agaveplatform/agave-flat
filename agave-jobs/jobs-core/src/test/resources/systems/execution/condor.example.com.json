{
	"id": "condor.example.com",
	"name": "Open Science Grid",
	"status": "UP",
	"type": "EXECUTION",
	"description": "Sample Condor system config",
	"site": "example.com",
	"executionType": "CONDOR",
	"default": false,
	"queues": [
        {
            "name": "condorqueue",
            "maxJobs": 5,
            "maxMemory": "1GB",
            "customDirectives": "",
            "defaut":true
        }
    ],
	"login": {
      "host": "${condor.host}",
      "port": ${condor.port},
		"protocol": "SSH",
		"auth":{
         "username":"testuser",
         "password":"testuser",
         "type":"PASSWORD"
      }
	},
	"storage":{
      "host": "${condor.host}",
      "port": ${condor.data.port},
      "protocol":"SFTP",
      "rootDir":"/",
      "homeDir":"/home/testuser",
      "publicAppsDir": "/tmp",
      "auth":{
         "username":"testuser",
         "password":"testuser",
         "type":"PASSWORD"
      }
    },
    "scratchDir": "/tmp",
	"workDir": "/tmp",
	"scheduler": "CONDOR",
	"environment": "",
	"startupScript": "./bashrc"
}