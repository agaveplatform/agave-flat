{
  "id": "ssh.example.com",
  "name": "SSH",
  "status": "UP",
  "type": "EXECUTION",
  "description": "Sample ssh fork execution system.",
  "site": "example.com",
  "executionType": "CLI",
  "default": false,
  "queues": [
    {
      "name": "normal",
      "maxJobs": 100,
      "maxMemory": "2048GB",
      "customDirectives": "#$ -A TG-MCB110022",
      "default": true
    }
  ],
  "login": {
    "host": "${ssh.host}",
    "port": ${ssh.port},
    "protocol": "SSH",
    "auth": {
      "username": "testuser",
      "password": "testuser",
      "type": "PASSWORD"
    }
  },
  "storage": {
    "host": "${ssh.host}",
    "port": ${ssh.data.port},
    "protocol": "SFTP",
    "rootDir": "/",
    "homeDir": "/home/testuser",
    "publicAppsDir": "/tmp",
    "auth": {
      "username": "testuser",
      "password": "testuser",
      "type": "PASSWORD"
    }
  },
  "scratchDir": "/tmp",
  "workDir": "/tmp",
  "scheduler": "FORK",
  "environment": "",
  "startupScript": "/home/testuser/.bashrc"
}