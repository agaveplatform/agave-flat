{
  "id": "s3.example.com",
  "name": "Amazon S3 Example",
  "status": "UP",
  "type": "STORAGE",
  "description": "Example storage system accessible via Amazon S3 API.",
  "site": "minio",
  "storage": {
    "host": "http://docker.example.com",
    "port": 9000,
    "protocol": "S3",
    "homeDir": "/",
    "rootDir": "/",
    "auth": {
      "publicKey": "${s3.access.key}",
      "privateKey": "${s3.secret.key}",
      "type": "APIKEYS"
    },
    "container": "${s3.bucket}"
  },
  "default": false
}